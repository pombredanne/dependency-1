GET     /_internal_/healthcheck                 controllers.HealthchecksController.index()
GET     /                                       controllers.ApplicationController.index()

GET     /libraries                              controllers.ApplicationController.redirect
GET     /libraries/                             controllers.LibrariesController.index(page: Int ?= 0)
GET     /libraries/:guid                        controllers.LibrariesController.show(guid: java.util.UUID, versionsPage: Int ?= 0, projectsPage: Int ?= 0)

GET     /languages                              controllers.ApplicationController.redirect
GET     /languages/                             controllers.LanguagesController.index(page: Int ?= 0)
GET     /languages/:guid                        controllers.LanguagesController.show(guid: java.util.UUID, projectsPage: Int ?= 0)

GET     /projects                               controllers.ApplicationController.redirect
GET     /projects/                              controllers.ProjectsController.index(page: Int ?= 0)
GET     /projects/create                        controllers.ProjectsController.create(repositoriesPage: Int ?= 0)
GET     /projects/createFromRepo                controllers.ProjectsController.createFromRepo(name: String, repositoriesPage: Int ?= 0)
GET     /projects/createAll                     controllers.ProjectsController.createAll()
GET     /projects/:guid                         controllers.ProjectsController.show(guid: java.util.UUID, languagesPage: Int ?= 0, librariesPage: Int ?= 0)
GET     /projects/:guid/edit                    controllers.ProjectsController.edit(guid: java.util.UUID)
POST    /projects/:guid/postEdit                controllers.ProjectsController.postEdit(guid: java.util.UUID)

GET     /search                                 controllers.SearchController.index(q: Option[String] ?= None, page: Int ?= 0)

GET     /login                                  controllers.ApplicationController.redirect
GET     /login/                                 controllers.LoginController.index(return_url: Option[String] ?= None)
GET     /login/github                           controllers.LoginController.githubCallback(code: String, state: Option[String] = None)
GET     /logout                                 controllers.LogoutController.index
GET     /logged_out                             controllers.LogoutController.logged_out

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
GET     /webjars/*file              controllers.WebJarAssets.at(file)